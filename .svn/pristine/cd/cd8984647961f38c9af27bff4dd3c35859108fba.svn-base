    <style name="TextBlueMedium">
        <item name="android:textSize">@dimen/text_14SP</item>
        <item name="android:textColor">@color/colorBlue</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="Base.Widget.Design.TabLayout" parent="android:Widget">
        <item name="tabBackground">@drawable/tab_background</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
        <!--<item name="tabIndicatorColor">#ff00ff</item>
        <item name="tabIndicatorHeight">2dp</item>-->
    </style>

    <style name="Text12Blue">
        <item name="android:textSize">@dimen/text_12SP</item>
        <item name="android:textColor">@color/colorBlue</item>
    </style>

    <style name="TextWhite12sp">
        <item name="android:textSize">@dimen/text_12SP</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="CustomTextAppearanceTab" parent="TextAppearance.Design.Tab">
        <item name="textAllCaps">false</item>
        <item name="android:textAllCaps">false</item>
    </style>

    <style name="CalenderViewCustom" parent="Theme.AppCompat">
        <item name="colorAccent">#3E92C2</item>
        <item name="colorPrimary">@android:color/darker_gray</item>
        <item name="android:textColorPrimary">@color/white</item>#FF8000
    </style>
    <style name="CalenderViewCustomPink" parent="Theme.AppCompat">
        <item name="colorAccent">@color/colorBrigadistPink</item>
        <item name="colorPrimary">@android:color/darker_gray</item>
        <item name="android:textColorPrimary">@color/white</item>#FF8000
    </style>

    <style name="CalenderViewCustomOrange" parent="Theme.AppCompat">
        <item name="colorAccent">#FF8000</item>
        <item name="colorPrimary">@android:color/darker_gray</item>
        <item name="android:textColorPrimary">@color/white</item>
    </style>

    <style name="CalenderViewCustom2" parent="Theme.AppCompat">
        <item name="colorAccent">#3E92C2</item>
        <item name="colorPrimary">@android:color/darker_gray</item>
        <item name="android:textColorPrimary">@color/colorBlack</item>
    </style>

    <style name="CalenderViewDateCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">@color/colorBlack</item>
    </style>

    <style name="CalenderViewWeekCustomText" parent="android:TextAppearance.DeviceDefault.Small">
        <item name="android:textColor">#3E92C2</item>
    </style>
    <style name="CalenderViewWeekCustomPinkText" parent="android:TextAppearance.DeviceDefault.Small">
